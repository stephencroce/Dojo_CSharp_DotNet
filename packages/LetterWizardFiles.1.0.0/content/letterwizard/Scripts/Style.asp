<STYLE>
  .TextTitle {font:bold 14pt Sans-Serif}

  .TextSmallNormal {font:normal 8pt Sans-Serif}
  .TextNormal {font:normal 10pt Sans-Serif}
  .TextLargeNormal {font:normal 12pt Sans-Serif}
  .TextXLargeNormal {font:normal 14pt Sans-Serif}
  .TextXXLargeNormal {font:normal 16pt Sans-Serif}
  .TextXXXLargeNormal {font:normal 18pt Sans-Serif}

  .TextSmallBold {font:Bold 8pt Sans-Serif}
  .TextBold {font:Bold 10pt Sans-Serif}
  .TextLargeBold {font:Bold 12pt Sans-Serif}
  .TextXLargeBold {font:Bold 14pt Sans-Serif} 
  .TextXXLargeBold {font:Bold 16pt Sans-Serif} 
  .TextXXXLargeBold {font:Bold 18pt Sans-Serif} 

  .CellTitle {border-bottom:solid 1px #000000}
  .CellLabel {
    white-space: nowrap;
    padding-left:10px;
    padding-right:5px;
    vertical-align:top;
  }
  .CellValue {width:100%;}
  .CellHeader {
    white-space: nowrap;
    border-bottom:solid 1px #000000;
  }
  .CellFooter {
    white-space: nowrap;
    border-top:solid 1px #000000;
  }
  .CellPaddingRight {padding-right:10px;}
  .CellPaddingLeft  {padding-left:10px;}
  .CellPaddingBoth  {padding-right:10px; padding-left:10px;}
  
  .PageSize_A4 {
    width:197mm;
    height:284mm;
  }
  .PageSize_Standard {
    width:203mm;
    height:266mm;
  }
  
  .Bullet {
    width:1px;
    white-space: nowrap;
    padding-left:10px;
    padding-right:5px;
    vertical-align:top;
  }
  
  .BulletContent {
    width:100%;
    padding-right:10px;
  }
  
  .Square {
    width:25px;
    height:25px;
    border:solid 1px #000000;
  }
  .SquareSpacer {
    font:normal 3pt Sans-Serif;
    padding-right:3px;
  }
  .SquareContent {
    font:normal 14pt Sans-Serif;
    padding-right:5px;
    padding-left:5px;
  }
  .SquareTextBox {
    padding:3px;
    border:solid 1px #000000;
  }
</STYLE>